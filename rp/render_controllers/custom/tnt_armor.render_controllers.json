{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.tnt_armor": {
			"geometry": "Geometry.default",
			"materials": [
				{
					"*": "variable.is_enchanted ? Material.enchanted : Material.default"
				}
			],
			"textures": ["(query.anim_time > 0.35) ? ((math.mod(query.anim_time, 0.7) < 0.35) ? Texture.starting : Texture.default) : Texture.default", "Texture.enchanted"],
			"part_visibility": [
				{
					"tnt": "!query.equipped_item_any_tag('slot.weapon.mainhand', 'ha:tnt')"
				}
			]
		}
	}
}
